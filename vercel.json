{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/debug-standalone",
      "destination": "/api/debug-standalone"
    },
    {
      "source": "/api/debug-js",
      "destination": "/api/debug-js"
    },
    {
      "source": "/api/auth/register",
      "destination": "/api/auth/register"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
